    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:background="@drawable/test_collection">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_island"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="섬의 마음"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_giant"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="거인의 심장"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_star"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="오르페우스의 별"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_picture"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="위대한 미술품"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_orgel"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="기억의 오르골"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_mococo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="모코코 씨앗"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_medal"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="이그네아의 징표"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_tree"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="세계수의 잎"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="10dp">

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:id="@+id/char_search_detail_collection_item_voyage"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="항해 모험물"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>

            <com.google.android.material.card.MaterialCardView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginRight="5dp"
                android:layout_weight="1"
                android:visibility="invisible">

                <com.lostark.loahelper.customview.CharSearchCollectionItemView
                    android:layout_marginLeft="5dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:collectionName="항해 모험물"></com.lostark.loahelper.customview.CharSearchCollectionItemView>
            </com.google.android.material.card.MaterialCardView>
        </LinearLayout>


    </LinearLayout>

    <FrameLayout
        android:id="@+id/char_search_detail_collection_list_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></FrameLayout>
</LinearLayout>